{
  // This file is not used in compilation. It is here just for a nice editor experience.
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"]
    },
    "types": ["node", "@docusaurus/module-type-aliases"],
    // other options...
  },
  "include": [
    "src/**/*",
    "src/@types/**/*",
    "docusaurus.config.ts",
    "sidebars.ts"
  ],
  "exclude": [".docusaurus", "build"]
}
